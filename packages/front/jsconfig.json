{
    "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "baseUrl": ".",
      "paths": {
        "~app/*": ["./src/app/*"],
        "~components/*": ["./src/app/components/*"],
        "~screens/*": ["./src/app/screens/*"],
        "~config/*": ["./src/config/*"],
        "~constants/*": ["./src/constants/*"],
        "~redux/*": ["./src/redux/*"],
        "~services/*": ["./src/services/*"],
        "~utils/*": ["./src/utils/*"],
        "~models/*": ["./src/models/*"],
        "~assets/*": ["./src/assets/*"],
        "~propTypes/*": ["./src/propTypes/*"],
        "~scss": ["./src/scss"]
      }
    },
    "include": ["src"],
    "exclude": ["node_modules", ".history"]
  }
  